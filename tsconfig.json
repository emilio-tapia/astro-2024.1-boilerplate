{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/assets": ["src/assets/*"],
      "@/components": ["src/components/*"],
      "@/data": ["src/data/*"],
      "@/i18n": ["src/i18n/*"],
      "@/icons": ["src/icons/*"],
      "@/layouts": ["src/layouts/*"],
      "@/lib": ["src/lib/*"],
      "@/pages": ["src/pages/*"],
      "@/store": ["src/store/*"],
      "@/styles": ["src/styles/*"],
      "@/utils": ["src/utils/*"],
      "@/vendor": ["src/vendor/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  // "files": ["node_modules/gsap/types/index.d.ts"],\
  "extends": "astro/tsconfigs/strictest"
}
